
{
    "version": 2,
    "env": {
        "NODE_OPTIONS": "--openssl-legacy-provider"
      },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "trailingSlash": true
  }
  